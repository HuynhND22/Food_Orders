{
    "version": 2,
    "builds": [
        {
            "src": "bin/www.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}