{
    "version": 2,
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        },
        {
            "src": "bin/www.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/bin/www.ts"
        }
    ]
}