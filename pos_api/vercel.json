{
    "version": 2,
    "builds": [
        {
            "src": "bin/www.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**",
                    "bin/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/bin/www.ts"
        }
    ]
}